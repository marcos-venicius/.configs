{
  "omnisharp.version": "v1.37.3",
  "omnisharp.trace.server": "information",
  "omnisharp.debug.server": true,
  "languageserver": {
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
